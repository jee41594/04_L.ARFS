package step01.one;

public class StaticTest {
	
	int a;
	static int b;
	
	StaticTest(){
		a++;
		b++;
	}
	
	static{
		System.out.println(2);
	}
	void m1(){
		System.out.println("m1()");
	}
	static void m2(){
		System.out.println("m2()");
	}

 	public static void main(String[] args) {
		StaticTest s = new StaticTest();
		
		System.out.println(s.a+b);

	}
	static{
		System.out.println(1);
	}
}
