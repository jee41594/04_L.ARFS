package step01.one;

public class Test2 {
	
	int a(int i, int j){
		
		return i+j;
	}
	int a(int i, int j, int k){
		
		return i+j+k;
	}
	
	void a1(int i, int j){
		
		System.out.println(i-j);
	}

	public static void main(String[] args) {
	Test2 t = new Test2();
	Test t1 = new Test();
	t.no1 = 100;
	System.out.println(t.no2+" "+t.no1);
